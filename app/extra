StructAccess
    = first:Id rest:( "." field:Id { return { base: first, field}; })* {
        return rest.reduce(
            (acc, curr) => {
                const { base, field } = curr
                return createNode('StructAccess', { base: acc, field })
            },
            createNode('VarValue', { id: first })
        );
    }
